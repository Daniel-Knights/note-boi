{
  "name": "note-boi",
  "version": "1.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Daniel-Knights/note-boi"
  },
  "type": "module",
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "vite build",
    "vite:preview": "vite preview",
    "dev": "tauri dev",
    "build": "tauri build",
    "icon": "pnpx @tauri-apps/tauricon ./artwork/icon.png",
    "info": "tauri info",
    "test": "vitest",
    "test:build": "vitest run",
    "lint": "eslint ./**/*.{ts,js,vue} --no-error-on-unmatched-pattern && vue-tsc --noEmit && tsc --noEmit",
    "checks": "pnpm run lint && pnpm run test:build",
    "format": "prettier --write ./src",
    "release:patch": "tsx ./scripts/release.ts patch",
    "release:minor": "tsx ./scripts/release.ts minor",
    "release:major": "tsx ./scripts/release.ts major"
  },
  "dependencies": {
    "@tauri-apps/api": "1.6.0",
    "quill": "^2.0.2",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@babel/types": "^7.24.8",
    "@tauri-apps/cli": "^1.6.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "2.4.6",
    "changenog": "^0.6.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.27.0",
    "fake-indexeddb": "^6.0.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.3",
    "quill-delta": "^5.1.0",
    "sass": "^1.77.8",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0",
    "vite": "^5.3.3",
    "vitest": "^1.6.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.26"
  }
}
