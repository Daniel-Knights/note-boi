{
  "name": "note-boi",
  "version": "1.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Daniel-Knights/note-boi"
  },
  "type": "module",
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "vite build",
    "vite:preview": "vite preview",
    "dev": "tauri dev",
    "build": "tauri build",
    "icon": "pnpx @tauri-apps/tauricon ./artwork/icon.png",
    "info": "tauri info",
    "test": "vitest",
    "test:build": "vitest run",
    "lint": "eslint ./**/*.{ts,js,vue} --no-error-on-unmatched-pattern && vue-tsc --noEmit && tsc --noEmit",
    "checks": "pnpm run lint && pnpm run test:build",
    "format": "prettier --write ./src",
    "release:patch": "tsx ./scripts/release.ts patch",
    "release:minor": "tsx ./scripts/release.ts minor",
    "release:major": "tsx ./scripts/release.ts major"
  },
  "dependencies": {
    "@tauri-apps/api": "1.6.0",
    "quill": "^2.0.2",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@babel/types": "^7.26.0",
    "@eslint/compat": "^1.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@tauri-apps/cli": "^1.6.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.9.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "2.4.6",
    "changenog": "^0.6.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.30.0",
    "fake-indexeddb": "^6.0.0",
    "globals": "^15.12.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "quill-delta": "^5.1.0",
    "sass": "^1.80.6",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.4",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.10"
  }
}
