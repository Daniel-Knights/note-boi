{
  "name": "note-boi",
  "version": "0.1.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/Daniel-Knights/note-boi"
  },
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "npm run lint && vite build",
    "vite:preview": "vite preview",
    "dev": "tauri dev",
    "build": "npm run test:build && tauri build",
    "icon": "npx @tauri-apps/tauricon ./artwork/icon.png",
    "info": "tauri info",
    "test": "vitest",
    "test:build": "vitest run",
    "lint": "eslint ./**/*.{ts,js,mjs,vue} --no-error-on-unmatched-pattern && vue-tsc --noEmit",
    "checks": "npm run lint && npm run test:build",
    "update": "node ./scripts/updater.mjs",
    "version:patch": "node ./scripts/version.mjs patch && npm version patch",
    "version:minor": "node ./scripts/version.mjs minor && npm version minor",
    "version:major": "node ./scripts/version.mjs major && npm version major",
    "release:patch": "npm run lint && npm run test:build && npm run version:patch && git push && git push --tags",
    "release:minor": "npm run lint && npm run test:build && npm run version:minor && git push && git push --tags",
    "release:major": "npm run lint && npm run test:build && npm run version:major && git push && git push --tags"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.1",
    "quill": "^1.3.7",
    "uuid": "^8.3.2",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@actions/github": "^5.0.3",
    "@babel/types": "^7.18.4",
    "@ianvs/prettier-plugin-sort-imports": "^3.4.2",
    "@tauri-apps/cli": "^1.0.0",
    "@types/node": "^18.0.0",
    "@types/quill": "^2.0.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.1",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "jsdom": "^20.0.0",
    "node-fetch": "^3.2.6",
    "prettier": "^2.7.1",
    "quill-delta": "^5.0.0",
    "sass": "^1.53.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.2",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^0.38.1"
  }
}
