{
  "name": "note-boi",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Daniel-Knights/note-boi"
  },
  "type": "module",
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "vite build",
    "vite:preview": "vite preview",
    "dev": "tauri dev",
    "build": "tauri build",
    "icon": "pnpx @tauri-apps/tauricon ./artwork/icon.png",
    "info": "tauri info",
    "test": "vitest",
    "test:build": "vitest run",
    "lint": "eslint ./**/*.{ts,js,vue} --no-error-on-unmatched-pattern && vue-tsc --noEmit && tsc --noEmit",
    "checks": "pnpm run lint && pnpm run test:build",
    "format": "prettier --write ./src",
    "update": "node ./scripts/updater.js",
    "release:patch": "tsx ./scripts/release.ts patch",
    "release:minor": "tsx ./scripts/release.ts minor",
    "release:major": "tsx ./scripts/release.ts major"
  },
  "dependencies": {
    "@tauri-apps/api": "1.5.0",
    "quill": "^1.3.7",
    "vue": "3.3.8"
  },
  "devDependencies": {
    "@actions/github": "^5.1.1",
    "@babel/types": "^7.23.4",
    "@tauri-apps/cli": "^1.5.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.9.5",
    "@types/quill": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "2.4.2",
    "changenog": "^0.5.2",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "fake-indexeddb": "^5.0.1",
    "jsdom": "^22.1.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.1.0",
    "quill-delta": "^5.1.0",
    "sass": "^1.69.5",
    "tsx": "^4.3.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vitest": "^0.34.6",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.22"
  }
}
