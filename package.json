{
  "name": "note-boi",
  "version": "0.1.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/Daniel-Knights/note-boi"
  },
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "vue-tsc --noEmit && vite build",
    "vite:preview": "vite preview",
    "dev": "tauri dev",
    "build": "npm run test:build && tauri build",
    "icon": "npx @tauri-apps/tauricon ./artwork/icon.png",
    "info": "tauri info",
    "test": "vitest",
    "test:build": "vitest run",
    "update": "node ./scripts/updater.mjs",
    "version:patch": "npm run test:build && node ./scripts/version.mjs patch && npm version patch",
    "version:minor": "npm run test:build && node ./scripts/version.mjs minor && npm version minor",
    "version:major": "npm run test:build && node ./scripts/version.mjs major && npm version major",
    "release:patch": "npm run version:patch && git push && git push --tags",
    "release:minor": "npm run version:minor && git push && git push --tags",
    "release:major": "npm run version:major && git push && git push --tags"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-rc.6",
    "quill": "^1.3.7",
    "uuid": "^8.3.2",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@actions/github": "^5.0.3",
    "@babel/types": "^7.18.4",
    "@ianvs/prettier-plugin-sort-imports": "^3.4.2",
    "@tauri-apps/cli": "^1.0.0-rc.13",
    "@types/node": "^17.0.41",
    "@types/quill": "^2.0.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.0",
    "jsdom": "^19.0.0",
    "node-fetch": "^3.2.5",
    "prettier": "^2.6.2",
    "quill-delta": "^5.0.0",
    "sass": "^1.52.2",
    "typescript": "^4.7.3",
    "vite": "^2.9.10",
    "vitest": "^0.14.1",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^0.37.3"
  }
}
