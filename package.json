{
  "name": "note-boi",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Daniel-Knights/note-boi"
  },
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "pnpm run lint && vite build",
    "vite:preview": "vite preview",
    "dev": "tauri dev",
    "build": "pnpm run test:build && tauri build",
    "icon": "pnpx @tauri-apps/tauricon ./artwork/icon.png",
    "info": "tauri info",
    "test": "vitest",
    "test:build": "vitest run",
    "lint": "eslint ./**/*.{ts,js,mjs,vue} --no-error-on-unmatched-pattern && vue-tsc --noEmit",
    "checks": "pnpm run lint && pnpm run test:build",
    "format": "prettier --write ./src",
    "update": "node ./scripts/updater.mjs",
    "release:patch": "tsx ./scripts/release.mts patch",
    "release:minor": "tsx ./scripts/release.mts minor",
    "release:major": "tsx ./scripts/release.mts major"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.5.0",
    "quill": "^1.3.7",
    "uuid": "^9.0.1",
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@actions/github": "^5.1.1",
    "@babel/types": "^7.23.0",
    "@tauri-apps/cli": "^1.5.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "^20.7.2",
    "@types/quill": "^2.0.11",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "2.4.1",
    "changenog": "^0.5.2",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.0.3",
    "quill-delta": "^5.1.0",
    "sass": "^1.68.0",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.6",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.15"
  }
}
